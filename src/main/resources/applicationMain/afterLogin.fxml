<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21"
            fx:controller="Controllers.LogOutController">
   <center>
       <AnchorPane fx:id="LODOBPicker" nodeOrientation="LEFT_TO_RIGHT" BorderPane.alignment="CENTER">
         <children>
             <AnchorPane layoutX="166.0" prefHeight="400.0" prefWidth="419.0">
               <children>
                   <ImageView fitHeight="226.0" fitWidth="208.0" layoutX="25.0" layoutY="54.0" pickOnBounds="true"
                              preserveRatio="true">
                     <image>
                         <Image url="@../Conan.png"/>
                     </image>
                  </ImageView>
                   <Button fx:id="Logout" layoutX="23.0" layoutY="297.0" mnemonicParsing="false" onAction="#userLogout"
                           prefHeight="33.0" prefWidth="208.0" text="Sign Out" textFill="#ff5e5e">
                     <font>
                         <Font name="AGaramondPro-BoldItalic" size="21.0"/>
                     </font>
                  </Button>
                   <Button fx:id="FMI" layoutX="23.0" layoutY="342.0" mnemonicParsing="false" onAction="#formore"
                           prefHeight="32.0" prefWidth="112.0" text="CPA SATISTIC" textFill="#ff5e5e">
                     <font>
                         <Font name="AGaramondPro-BoldItalic" size="9.0"/>
                     </font>
                  </Button>
                   <Button fx:id="FYI" layoutX="139.0" layoutY="342.0" mnemonicParsing="false"
                           onAction="#forinformation" prefHeight="32.0" prefWidth="91.0" text="My information"
                           textFill="#ff5e5e">
                     <font>
                         <Font name="AGaramondPro-BoldItalic" size="9.0"/>
                     </font>
                   </Button>
                   <ComboBox fx:id="SUAchoice" layoutX="255.0" layoutY="108.0" prefWidth="150.0" visible="false"/>
                   <Text fx:id="SUAtext" layoutX="255.0" layoutY="103.0" strokeType="OUTSIDE" strokeWidth="0.0"
                         text="Set up admin privileges" visible="false">
                       <font>
                           <Font name="Times New Roman Italic" size="8.0"/>
                       </font>
                   </Text>
                   <Button fx:id="SUAsetupbutton" layoutX="304.0" layoutY="138.0" mnemonicParsing="false"
                           onAction="#SUAsetup" text="Set up" visible="false">
                       <font>
                           <Font name="System Bold" size="12.0"/>
                     </font>
                  </Button>
               </children>
               <effect>
                   <Blend/>
               </effect>
            </AnchorPane>
             <Accordion layoutX="283.0" layoutY="62.0"/>
             <Label fx:id="LOuserwel" contentDisplay="CENTER" layoutX="14.0" layoutY="14.0" text="Label"
                    textAlignment="CENTER" visible="false">
                 <font>
                     <Font name="Times New Roman Bold Italic" size="15.0"/>
                 </font>
                 <textFill>
                     <LinearGradient endX="1.0" endY="1.0">
                         <stops>
                             <Stop>
                                 <color>
                                     <Color/>
                                 </color>
                             </Stop>
                             <Stop offset="1.0">
                                 <color>
                                     <Color red="1.0"/>
                                 </color>
                             </Stop>
                         </stops>
                     </LinearGradient>
                 </textFill>
             </Label>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
